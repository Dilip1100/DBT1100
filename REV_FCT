CREATE TABLE IF NOT EXISTS FCT_REVIEWS AS
SELECT
    *
FROM

AIRBNB.RAW.RAW_REVIEWS 

WHERE
COMMENTS IS NOT NULL;
INSERT INTO FCT_REVIEWS
SELECT
    *
FROM
    AIRBNB.RAW.RAW_REVIEWS
WHERE
    COMMENTS IS NOT NULL
    AND DATE > (
        SELECT MAX(DATE) 
        FROM AIRBNB.RAW.RAW_REVIEWS
    );
